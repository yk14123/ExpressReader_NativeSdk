<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#131f27" />
        </shape>
    </item>
    <item>
        <rotate
            android:fromDegrees="-35"
            android:pivotX="0%"
            android:pivotY="-0%"
            android:toDegrees="-5">
            <shape android:shape="rectangle">
                <solid android:color="#192934" />
            </shape>
        </rotate>
    </item>
</layer-list>